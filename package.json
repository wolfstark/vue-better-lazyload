    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "vue": "^2.5.13",
    "webpack": "^3.6.0"
  },
  "typings": "typings/index.d.ts",
  "peerDependencies": {
    "vue": "^2.5.13"
