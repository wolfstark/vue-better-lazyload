  "license": "MIT",
  "main": "dist/vue-better-lazyload.js",
  "unpkg": "dist/vue-better-lazyload.js",
  "scripts": {
    "build:umd": "node build.js",
    "build:examples": "cp -rv ./dist/ ./examples/src/",
    "upgrade:examples": "cd ./examples && yarn upgrade vue-better-lazyload",
